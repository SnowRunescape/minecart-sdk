{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "outDir": "dist",
        "skipLibCheck": true,
        "strict": true,
        "strictFunctionTypes": true,
        "target": "ES2022",
        "baseUrl": ".",
        "paths": {
            "@Minecart/*": [
                "./src/*"
            ],
        },
        "rootDir": "src"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}
